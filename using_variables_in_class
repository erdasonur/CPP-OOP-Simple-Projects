#include <iostream>
#include <string>
using namespace std;

class Class1{
      private:
          string name;
      public:
          void setName(string name){
              this.name=name;
              }
          string getName(){
               return name;
              }
   };
   
   int main(){
        Class1 object;
        string name ="Adam William";
        object.setName(name);
        cout << object.getName() << endl;
        return 0;
   }
